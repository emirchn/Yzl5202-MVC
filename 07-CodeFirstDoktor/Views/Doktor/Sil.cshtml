@model Doktor
<div class="row">
	<form method="post" asp-action="Sil">
		Ad: <input type="text" asp-for="Ad" />
		Soyad: <input type="text" asp-for="Soyad" />
		Email: <input type="text" asp-for="Email" />
		Brans: <input type="text" asp-for="Brans" />
		Doğum Tarihi: <input type="date" asp-for="DogumTarihi" />
		<input type="hidden" asp-for="Id" />
		<input type="submit" value="Sil" class="btn btn-danger" />
	</form>
</div>